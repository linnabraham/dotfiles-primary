        rofi-screenshot
super + S
        rofi-screenshot -s
super + T
        mpdmenu.sh
super + u
	wrapper-wmctrl.sh start-plattenalbum
super + w
        switch-i3-workspace.sh
super + BackSpace
        sysact
super + slash
        mpc_control -t
super + question
        dmenu-wrapper-music
super + colon
        gtk-launch st-calc
alt + XF86Favorites
	playerctl play-pause
alt + question
	hotkey-helper.sh
shift + Prior
	mpc prev
shift + Next
	mpc next
F7
        mpc toggle
